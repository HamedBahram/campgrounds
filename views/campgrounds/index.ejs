<%- include('../partials/_header') %>

    <main class="container">
        <h1>All Campgrounds</h1>
        <div>
            <a href="/campgrounds/new">Add a Camp</a>
        </div>
        <ul>
            <% campgrounds.forEach(camp => { %>
            <li><a href="/campgrounds/<%= camp._id %>"><%= camp.title  %></a></li>
            <% }) %>
        </ul>
    </main>

<%- include('../partials/_footer') %>